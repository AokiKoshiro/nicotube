{
    "name": "nicotube",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["jquery-3.6.0.min.js", "TweenMax.min.js", "content.js"],
        "css": ["content.css"]
    }],
    "icons": {
        "48": "icons/icon48.png",
        "96": "icons/icon96.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage"]
}